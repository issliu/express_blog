<head>
    <title><%= title %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="styles/style.css">
    <link rel="stylesheet" href="styles/normalize.css">
</head>

<header class="header">
    <% if (user) { %>
    <span class="menu"><a title="登出" href="/logout">logout</a></span>
        <span class="menu"><a title="发表" href="/post">post</a></span>
    <% } else { %>
    <span class="menu"><a title="注册" href="/reg">register</a></span>
        <span class="menu"><a title="登录" href="/login">login</a></span>
    <% } %>
    <span class="menu">
        <a title="主页" href="/">home</a>
    </span>
</header>

<% if (success) { %>
<div><%= success %></div>
<% } %>
<% if (error) { %>
<div><%= error %> </div>
<% } %>
