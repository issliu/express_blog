<!DOCTYPE html>
<html>
<%- include header %>
<body>
<% posts.forEach(function (post, index) { %>
    <div class="post">
        <h2 class="title">
            <a href="#"><%= post.title %></a>
        </h2>
        <p class="info">
            作者：<a href="#"><%= post.name %></a> | 日期：<%= post.time.minute %>
        </p>
        <div class="content">
            <%- post.post %>
        </div>
    </div>
<% }) %>
<%- include footer %>
</body>
</html>
